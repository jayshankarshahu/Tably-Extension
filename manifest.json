{
    "manifest_version": 3,
    "name": "Tably",
    "version": "1.0.0",
    "description": "Simulate Alt+Tab behaviour for your chrome tabs",
    "action": {
      "default_popup": "src/popup/popup.html",
    "default_icon": {
      "16": "public/tably.png",
      "32": "public/tably.png",
      "48": "public/tably.png",
      "128": "public/tably.png"
    },
      "icons": {
    "16": "public/tably.png",
    "32": "public/tably.png",
    "48": "public/tably.png",
    "128": "public/tably.png"
    }
    },
    "permissions": ["tabs","scripting","activeTab"],
    "background" : {
        "service_worker": "src/background.js"
    },
    "commands": {
        "open_content_script": {
        "suggested_key": {
            "default": "Alt+Q"
        },
        "description": "Run content script"
        }
    }
  }