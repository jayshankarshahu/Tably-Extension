{
    "manifest_version": 3,
    "name": "Tably",
    "version": "1.0.0",
    "description": "Simulate Alt+Tab behaviour for your chrome tabs",
    "action": {
      "default_popup": "src/popup/popup.html"
    },
    "permissions": ["tabs","scripting","activeTab"],
    "background" : {
        "service_worker": "src/background.js"
    },
    "commands": {
        "open_content_script": {
        "suggested_key": {
            "default": "Alt+Q"
        },
        "description": "Run content script"
        }
    }
  }